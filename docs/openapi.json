  "info": {
    "title": "360Solve Connector API",
    "description": "Multi-tenant ERP Integration Platform API",
    "version": "1.0.0",
    "contact": {

    }
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      },
      "tenantId": {
        "type": "apiKey",
        "in": "header",
        "name": "X-Tenant-ID"
      }
    },
    "schemas": {
      "SignUpDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@company.com"
          },
          "password": {
            "type": "string",
            "example": "SecurePass123!"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          },
          "companyName": {
            "type": "string",
            "example": "Acme Corp"
          },
          "tier": {
            "type": "string",
            "example": "free",
            "enum": [
              "free",
              "standard",
              "enterprise"
            ]
          }
        },
        "required": [
          "email",
          "password",
          "name",
          "companyName",
          "tier"
        ]
      },
      "SignInDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@company.com"
          },
          "password": {
            "type": "string",
            "example": "SecurePass123!"
          },
          "rememberMe": {
            "type": "boolean",
            "example": false
          }
        },
        "required": [
          "email",
          "password",
          "rememberMe"
        ]
      },
      "RefreshTokenDto": {
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string"
          }
        },
        "required": [
          "refreshToken"
        ]
      },
      "TenantAggregatorResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "aggregatorId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "aggregatorName": {
            "type": "string"
          },
          "aggregatorDescription": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "logoUrl": {
            "type": "string"
          },
          "configSchema": {
            "type": "object"
          },
          "status": {
            "type": "string"
          },
          "config": {
            "type": "object"
          },
          "hasCredentials": {
            "type": "boolean"
          },
          "lastTestAt": {
            "format": "date-time",
            "type": "string"
          },
          "lastTestStatus": {
            "type": "string"
          },
          "lastTestError": {
            "type": "string"
          },
          "lastSyncAt": {
            "format": "date-time",
            "type": "string"
          },
          "miniConnectorId": {
            "type": "string"
          },
          "installedAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "aggregatorId",
          "name",
          "aggregatorName",
          "description",
          "category",
          "type",
          "status",
          "config",
          "hasCredentials",
          "installedAt",
          "updatedAt"
        ]
      },
      "TenantAggregatorListResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TenantAggregatorResponseDto"
            }
          }
        },
        "required": [
          "success",
          "data"
        ]
      },
      "TenantAggregatorDetailResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/TenantAggregatorResponseDto"
          }
        },
        "required": [
          "success",
          "data"
        ]
      },
      "InstallTenantAggregatorDto": {
        "type": "object",
        "properties": {
          "aggregatorId": {
            "type": "string"
          },
          "marketplaceId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "config": {
            "type": "object"
          },
          "credentials": {
            "type": "object"
          },
          "testConnection": {
            "type": "boolean",
            "default": false
          }
        },
        "required": [
          "name"
        ]
      },
      "SaveTenantAggregatorCredentialsDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "config": {
            "type": "object"
          },
          "credentials": {
            "type": "object"
          }
        },
        "required": [
          "credentials"
        ]
      },
      "SchemaDiscoveryResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "success",
          "data"
        ]
      },
      "PreviewTableDto": {
        "type": "object",
        "properties": {
          "limit": {
            "type": "number",
            "default": 10,
            "minimum": 1,
            "maximum": 100
          }
        }
      }
    }
  }
}