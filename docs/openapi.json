  "info": {
    "title": "360Solve Connector API",
    "description": "Multi-tenant ERP Integration Platform API",
    "version": "1.0.0",
    "contact": {

    }
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      },
      "tenantId": {
        "type": "apiKey",
        "in": "header",
        "name": "X-Tenant-ID"
      }
    },
    "schemas": {
      "SignUpDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@company.com"
          },
          "password": {
            "type": "string",
            "example": "SecurePass123!"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          },
          "companyName": {
            "type": "string",
            "example": "Acme Corp"
          },
          "tier": {
            "type": "string",
            "example": "free",
